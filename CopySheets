Sub CopySheets()

Application.ScreenUpdating = False

Dim weeklyReportWorkbook As Workbook
Dim finalReportWorkbook As Workbook

Set finalReportWorkbook = Workbooks("201905_EMH.xlsx")

Dim weeklyReportSheet As Worksheet
Dim Fileslist as Range
Dim File as Range
Set Fileslist = Range("FilesList")

For Each File In Fileslist
Workbooks.Open File
Set Filename = ActiveWorkbook.Name
Sheets(2).Copy After:=finalReportWorkbook.Sheets(finalReportWorkbook.Sheets.Count)
With finalReportWorkbook.Sheets(finalReportWorkbook.Sheets.Count).UsedRange
.Value = .Value

End With
Workbooks(Filename).Close SaveChanges:=False
next

Application.ScreenUpdating = True

End Sub